function detailInfo(){
	argUrl = "/searchPerInfo/";
	radomT = "?_=" + (new Date()).getTime();
	var url = E3CommonsVariables.RequestPrefixQuery() + argUrl + radomT;
	jQuery.ajax({
		url : url,
		type : "POST",
		data : {},
		async : true,
		dataType : "JSON",
		contentType : "application/x-www-form-urlencoded;charset=UTF-8",
		success : function(data){
			//标头
			if(data.userInfo.loginType=="05"){
				
				if(data.userInfo.userNettype == "fix" && data.result.MyDetail != null &&  data.result.MyDetail.usernumber != null){
					$("#title_usernumber").html(data.result.MyDetail.usernumber)
				}else{
					$("#title_usernumber").html(data.userInfo.mobile)
				}
			}else{
				$("#title_usernumber").html(data.userInfo.mobile)
			}
			if(data.result.MyDetail==undefined){
				return
			}
			if(data.userInfo.userNettype=="fix"){
				$("#title_custname").html(data.result.MyDetail.accname);
				$("#my_package_leavePackage").html("");
			}else{
				$("#title_custname").html(data.result.MyDetail.custname);
			}
			/*if(data.userInfo.userNettype=="fix"){
				
				$("#title_custname").html(data.result.MyDetail.accname)
			}else{
				$("#title_custname").html(data.result.MyDetail.custname)
			}
*/
			if(data.userInfo.userNettype=="fix"){
				$("#my_package_subscrbstat").html(data.result.MyDetail.subscrbstat)
				$("#my_package_custname").html(data.result.MyDetail.accname)
				$("#my_package_landlvl").html(data.result.MyDetail.landlvl)
				$("#my_package_roamstat").html(data.result.MyDetail.roamstat)
				$("#my_package_opendate").html(data.result.MyDetail.opendate)
				$("#my_package_brand").html(data.result.MyDetail.brand);
				$("#my_package_payType").html(data.result.payType)
			}else{
				$("#my_package_subscrbstat").html(data.result.MyDetail.subscrbstat)
				$("#my_package_custname").html(data.result.MyDetail.custname)
				$("#my_package_landlvl").html(data.result.MyDetail.landlvl)
				$("#my_package_roamstat").html(data.result.MyDetail.roamstat)
				$("#my_package_opendate").html(data.result.MyDetail.opendate)
				$("#my_package_brand").html(data.result.MyDetail.brand)
				$("#my_package_payType").html(data.result.payType);
				$("#my_package_pukcode").html(data.result.pukcode)
			}
			
			//我的套餐
			if(data.userInfo.userNettype== "4G"){
				
				if(data.result.fourPackage != null && data.result.fourPackage.productinfo[0].productname != null){
					$("#my_package_productname").html(data.result.fourPackage.productinfo[0].productname)
				}else{
					$("#my_package_productname").html(data.userInfo.packageName)
				}
				
				if(data.result.fourPackage != null && data.result.fourPackage.productinfo[0].productfee != null && data.result.fourPackage.productinfo[0].productfee != "null"){
					$("#my_package_packageFee").html(data.result.fourPackage.productinfo[0].productfee+'元/月')
				}else{
					if(data.userInfo.packageFee != null){
						$("#my_package_packageFee").html(data.userInfo.packageFee)
					}else{
						$("#my_package_packageFee").html('以套餐资费为准')
					}
				}
			}else if(data.userInfo.userNettype== "3G" ){
				$("#my_package_productname").html(data.userInfo.packageName)
				if(data.userInfo.packageFee != null){
					$("#my_package_packageFee").html(data.userInfo.packageFee)
				}else{
					$("#my_package_packageFee").html('以套餐资费为准')
				}
			}else if(data.userInfo.userNettype== "2G"){
				if(data.userInfo.packageName!= null&&data.userInfo.packageName!= ""){
					$("#my_package_productname").html(data.userInfo.packageName);
					$("#my_package_packageFee").html('以套餐资费为准')
				}else{
					$("#my_package_productname").html("2G套餐无法显示,请升级4G套餐.");
					$("#my_package_packageFee").html('以套餐资费为准')
				}
				
			}else if(data.userInfo.userNettype== "fix"){
				$("#my_package_productname").html(data.userInfo.packageName);
				$("#my_package_packageFee").html('以套餐资费为准')
			}
			
			

			//我的状态开通4G网络
			if(detail_cache==undefined){
				detail_cache = new Object();
			}
			//我的套餐
			if(data.userInfo.userNettype== "4G"){
				detail_cache.openfournet="4G";
			}else if(data.userInfo.userNettype== "3G" ){
				//我的状态开通4G网络
				$("#openfournet_state").show()
			}else if(data.userInfo.userNettype== "2G"){
				//我的状态开通4G网络
				$("#openfournet_state").show()
			}
			
			if(data.userInfo.userNettype=="fix"){
				$("#my_package_subscrbstat").html(data.result.MyDetail.subscrbstat)
				$("#my_package_custname").html(data.result.MyDetail.accname)
				$("#my_package_landlvl").html(data.result.MyDetail.landlvl)
				$("#my_package_roamstat").html(data.result.MyDetail.roamstat)
				$("#my_package_opendate").html(data.result.MyDetail.opendate)
				$("#my_package_brand").html(data.result.MyDetail.brand);
				$("#my_package_payType").html(data.result.payType)
			}else{
				$("#my_package_subscrbstat").html(data.result.MyDetail.subscrbstat)
				$("#my_package_custname").html(data.result.MyDetail.custname)
				$("#my_package_landlvl").html(data.result.MyDetail.landlvl)
				$("#my_package_roamstat").html(data.result.MyDetail.roamstat)
				$("#my_package_opendate").html(data.result.MyDetail.opendate)
				$("#my_package_brand").html(data.result.MyDetail.brand)
				$("#my_package_payType").html(data.result.payType);
				$("#my_package_pukcode").html(data.result.pukcode)
			}
			
			
			//我的信息
			if(data.userInfo.loginType=="05"){
				// 判断我的其他号码显示还是隐藏  规则： 只有手机号码显示
				if(data.userInfo.userNettype !="fix"){
					$("#Myothernum").show();
				}else{
					$("#Myothernum").hide();
				}
				if(data.userInfo.userNettype == "fix" && data.result.MyDetail != null &&  data.result.MyDetail.usernumber != null){
					$("#my_info_usernumber").html(data.result.MyDetail.usernumber)
				}else{
					$("#my_info_usernumber").html(data.userInfo.mobile +" ")
				}
			}else{
				// 判断我的其他号码显示还是隐藏  规则： 只有手机号码显示
				if(data.userInfo.userNettype !="fix"){
					$("#Myothernum").show();
				}else{
					$("#Myothernum").hide();
				}
				$("#my_info_usernumber").html(data.userInfo.mobile  +" ")
			}
			//客户经理信息
			if(data && data.result != null&& data.result.MyDetail != null &&  data.result.MyDetail.managername){
				$("#my_info_managername").html(data.result.MyDetail.managername);
			}else{
				$("#my_info_managername").html("无");
			}
			if(data && data.result != null&& data.result.MyDetail != null &&  data.result.MyDetail.managercontact){
				$("#my_info_managercontact").html(data.result.MyDetail.managercontact);
			}else{
				$("#my_info_managercontact").html("10018");
			}
			
			if(data.userInfo.userNettype != "" && data.userInfo.userNettype != "fix"){
				if(data.result.MyDetail.custlvl != null && data.result.MyDetail.custlvl !=""){
					custlv1=data.result.MyDetail.custlvl
				}else{
					custlv1="未评级"
				}
			}else{
				custlv1="未评级"
			}
			if(custlv1== "钻石级"){
				$("#detailed_custlvl").html('<span>用户等级：</span><i><img src="//m2.img.10010.com/e4/images/purple.png"></i><a href="/e4/query/data_level.html" target="view_frame" style="float:right ">等级说明</a>')
			}else if(custlv1== "金级"){
				$("#detailed_custlvl").html('<span>用户等级：</span><i><img src="//m2.img.10010.com/e4/images/golden.png"></i><a href="/e4/query/data_level.html" target="view_frame" style="float:right ">等级说明</a>')
			}else if(custlv1== "银级"){
				$("#detailed_custlvl").html('<span>用户等级：</span><i><img src="//m2.img.10010.com/e4/images/yin.png"></i><a href="/e4/query/data_level.html" target="view_frame" style="float:right ">等级说明</a>')
			}else if(custlv1== "忠诚用户"){
				$("#detailed_custlvl").html('<span>用户等级：</span><i><img src="//m2.img.10010.com/e4/images/HEART.png"></i><a href="/e4/query/data_level.html" target="view_frame" style="float:right ">等级说明</a>')
			}else if(custlv1== "贵宾用户"){
				$("#detailed_custlvl").html('<span>用户等级：</span><i><img src="//m2.img.10010.com/e4/images/vip.png"></i><a href="/e4/query/data_level.html" target="view_frame" style="float:right ">等级说明</a>')
			}else if(custlv1== "贵宾忠诚用户"){
				$("#detailed_custlvl").html('<span>用户等级：</span><i><img src="//m2.img.10010.com/e4/images/vip.png"></i><i><img src="//m2.img.10010.com/e4/images/HEART.png"></i><a href="/e4/query/data_level.html" target="view_frame" style="float:right ">等级说明</a>')
			}else if(custlv1== "一星用户"){
				$("#detailed_custlvl").html('<span>用户等级：</span><i><img src="//m2.img.10010.com/e4/images/star.png"></i><a href="/e4/query/data_level.html" target="view_frame" style="float:right ">等级说明</a>')
				$("#my_info_Rank_p").show();
