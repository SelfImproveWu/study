// 温馨提示语
function currentTime() {
	var hour = new Date().getHours();
	if (hour >= 6 && hour < 11) {
		return "上午好";
	} else if (hour >= 11 && hour < 13) {
		return "中午好";
	} else if (hour >= 13 && hour < 19) {
		return "下午好";
	} else {
		return "晚上好";
	}
}
// 订单信息
var initOrderInfo = function(data) {
    var num = data.TOTALNUM;
    if (num == 0) {
        $(".money_w3").html("<p>您没有未支付订单，赶紧去订购吧!</p>");
        return false;
    } else {
        var tailHtml = '<center>共<b>' + num + '</b>条订单&#12288;共计<strong>￥' + formatPrice(data.SUMMONEY, 2) + '</strong></center><br>';
        var li = "";
        var orderList = data.ORDERGOODS_LIST;
        if (orderList.length > 0) {
            $(".money_w3>p").hide();
            $(".data_w").show();
        }
        if (orderList.length > 2) {
            $(".more_w").show();
        }
        if (orderList.length > 1) {
	        for (var i = 0; i < 2; i++) {
	            li += '<div class="main_wf">' 
	                + '<span class="ig_w"><img src="'+orderList[i].PHOTO_URL+'" alt="'+orderList[i].GOODS_NAME+'" width="50" height="50"/></span>'
	                + '<span class="jg_w">'+orderList[i].GOODS_NAME+'</span>'
	                + '<span class="nm_w">￥' + formatPrice(orderList[i].INCOME_MONEY, 2) + '</span>'
	                + '</div>';
	        }
        } else {
            li += '<div class="main_wf">' 
                + '<span class="ig_w"><img src="'+orderList[0].PHOTO_URL+'" alt="'+orderList[0].GOODS_NAME+'" width="50" height="50"/></span>'
                + '<span class="jg_w">'+orderList[0].GOODS_NAME+'</span>'
                + '<span class="nm_w">￥' + formatPrice(orderList[0].INCOME_MONEY, 2) + '</span>'
                + '</div>';
        }
        $(".gouwu_text").html(li);
        $(".nav_w").html(tailHtml);
        
    }
    $(".money_w1").html('未付款订单（' + num + '）');
};
// 登录框  
function log_out() {
    var params = {};
    jQuery.getJSON('//uac.10010.com/portal/Service/LogOut?callback=?', params, function() {
    	window.location.reload();
});
}
