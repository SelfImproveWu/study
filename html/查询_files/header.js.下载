				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/vip.png"></i><i><img src="//m2.img.10010.com/e4/images/HEART.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i>')
			}
			if(custlv1=="无"){
				$("#menu_custlvl_message").html("用户等级："+custlv1)
			}else{
				$("#menu_custlvl_message").html(custlv1)
			}
			//2015年12月11日注掉我的勋章，未知暂空
		/*	if(data.userInfo.userNettype!= "fix"){
				userNettype = data.userInfo.userNettype
				is_vip = data.userInfo.is_vip
				if(userNettype == "4G"){
					$("#my_detail_userNettype").html('<img src="//m2.img.10010.com/e4/images/wo_vip_img2.jpg">')
				}
				
				if(is_vip != null && is_vip != ""){
					$("#my_detail_is_vip").html('<img src="//m2.img.10010.com/e4/images/wo_vip_img4.jpg">')
				}
			}*/
			//我的资料
			if(data.userInfo.userNettype=="fix"){
				var sex = data.result.MyDetail.custsex;
				$("#my_data_custname").html(data.result.MyDetail.accname);
				if(sex==""|| sex==undefined){
					$("#custsex").html("不详");
				}else{
					$("#custsex").html(sex);
				}
				var num =data.result.MyDetail.usernumber;
				if(num==""||num==undefined){
					$("#my_data_usernumber").html("无");
				}else{
					$("#my_data_usernumber").html(num);
				}
				var cert = data.result.MyDetail.certaddr;
				if(cert ==""||cert==undefined){
					$("#certaddr").html("无");
				}else{
					$("#certaddr").html(cert);
				}
			}else{
				var sex = data.result.MyDetail.custsex;
				$("#my_data_custname").html(data.result.MyDetail.custname)
				if(sex==""|| sex==undefined){
					$("#custsex").html("不详");
				}else{
					$("#custsex").html(sex);
				}
				
				var num =data.result.MyDetail.usernumber;
				if(num==""||num==undefined){
					$("#my_data_usernumber").html("无");
				}else{
					$("#my_data_usernumber").html(num);
				}
				var cert = data.result.MyDetail.certaddr;
				if(cert ==""||cert==undefined){
					$("#certaddr").html("无");
				}else{
					$("#certaddr").html(cert);
				}
			}
			var id_card;
			if(data.result.MyDetail.certtype ==undefined){
				id_card = data.result.MyDetail.certnum
			}else{
				id_card = data.result.MyDetail.certtype+data.result.MyDetail.certnum
			}
			$("#certtype_certnum").html(id_card);
			$("#service_password").html('*******<a href="/e4/transact/basic/transact_password-iframe.html">修改密码</a>');
		}
	});
}
//点击头像旁边的个人信息跳转
function tiaozhuan(){
	window.location.href='http://iservice.10010.com/e4/query/basic/personal_xx_iframe.html';
}

function switch_modal(oldmodal,newmodal){
	$('#'+oldmodal).modal('hide')
	$('#'+newmodal).modal('show')
}

function close_iframe(fun){
	if(fun=='sureFun'){
		document.getElementById("ifram_eavatar_editor").contentWindow.sureFun()
	}else if(fun=='deleteLocalTempImage'){
		document.getElementById("ifram_eavatar_editor").contentWindow.deleteLocalTempImage()
		switch_modal('avatar-editor','profile-modal')
	}
}

//未读消息数量
function setNoReadMsgCount(){
	$.ajax({
		//url: E3CommonsVariables.RequestPrefix() + '/query/message/noReadMsgCount'+ "?_=" + (new Date()).getTime(),
		//url: E3CommonsVariables.RequestPrefix() + '/query/message/navNoReadAndNewMsg'+ "?_=" + (new Date()).getTime(),
		url: E3CommonsVariables.RequestPrefix() + '/query/message/pageList'+ "?_=" + (new Date()).getTime(),
		dataType:'JSON',
		data:{
			//active:'active',
			//count:2,
			currentPage: 1,
			pageSize: 2
			
		},
		success: function(data){
			//console.log(JSON.stringify(data));
			$('#header-msg-no-read-count').text(data.noReadCount);
			$('#msg-list-size').val(data.canReadCount);
			//展示两条最新消息
			var htmlMsg = '';
			if(data.pageList && data.pageList.length){
				$.each(data.pageList, function(){
					//console.log(JSON.stringify(this));
					htmlMsg += '<dt>'+ this.pushStartTime +'</dt><dd>|&nbsp;'+ this.msgContent +'</dd>';
				});
				htmlMsg += '<dd><a role="button" class="nav-list-item" title="消息" href="/e4/query/information.html">更多>></a></dd>';
			}
			$('#header-msg-new').html(htmlMsg);
			$('#msg-list-size').val(data.msgListSize | 0);
			
		}
	});
	//$('#header-msg-no-read-count').text(11);
	//$('#msg-list-size').val(18);
}

//我的状态
var my_state = {
		init : function(){
			if(detail_cache==undefined){
				detail_cache = new Object();
			}
			
			if(detail_cache.long==null){
				my_state.longinit()
			}
			if(detail_cache.roam==null){
				my_state.roaminit()
			}
			if(detail_cache.forward==null){
				my_state.forwardinit()
			}
			if(detail_cache.openfournet==null){
				my_state.openfournetinit()
			}
			
		},
		longinit : function (){
			TransactMain.loadData("/querylong",null,"my_state.longsucessQuery(data)");
		},
		longsucessQuery : function(data){
			detail_cache.long=data;
			if(data.isLongError=="true"){
				$("#long_state").html("无")
			}else{
				if(data.queryInfo.openflag=="1"){
					$("#long_state").html("已开通")
					
				}else if(data.queryInfo.openflag=="0"){
					$("#long_state").html("已关闭")
					
				}
				
				if(data.queryInfo.limitFlag==true){
					if(data.queryInfo.openflag=="0"){
						$("#long_state").html("已关闭")
					}else if(data.queryInfo.openflag=="1"){
						$("#long_state").html("已开通")
					}else{
						$("#long_state").html("无")
					}
				}else{
					$("#long_state").html("无")
				}
			}
			
		},
		roaminit : function(){
			TransactMain.loadData('/roamquery',null,'my_state.roamsucessQuery(data)');
		},
		roamsucessQuery : function(data){
			detail_cache.roam=data;
			if(!data.rspArgs.success){
				$("#roam_state").html("无")
			}else{
				if(data.rspArgs.openflag=='1'){
					$("#roam_state").html("已开通")
					//$("#roam_state p:nth-child(1) a").html("关 闭")
				}else if(data.rspArgs.openflag=='0'){
					$("#roam_state").html("已关闭")
					//$("#roam_state p:nth-child(1) a").html("开 通")
				}else{
					$("#roam_state").html("无")
				}
			}
		},
		forwardinit : function(data){
			TransactMain.loadData("/forwardInfo/", null,'my_state.forwardsucessQuery(data)');
			
		},
		forwardsucessQuery : function(data){
			detail_cache.forward=data;
			if(data.forwardInfoResult!=null){
				if (data.forwardInfoResult.forwardlist != null) {
					if(data.forwardInfoResult.forwardlist.length>1){
						$("#forward_state").html("已开通")
						//$("#forward_state p:nth-child(1) a").html("关 闭")
					}else{
						$("#forward_state").html("已关闭")
						//$("#forward_state p:nth-child(1) a").html("开 通")
					}
				}else{
					$("#forward_state").html("无")
				}
			}else{
				$("#forward_state").html("无")
			}
		},
		openfournetinit : function(){
			TransactMain.loadData("/openfourginternetquery/",null,'my_state.openfournetsucessQuery(data)');
		},
		openfournetsucessQuery : function(data){
			detail_cache.openfournet=data;
			if(data.issuccess){
				if(data.datanetworktype != null){
					if(data.datanetworktype=='4'){
						$("#openfournet_state span:nth-child(2)").html("已开通")
						//$("#openfournet_state p:nth-child(1) a").html("关 闭")
					}else{
						$("#openfournet_state span:nth-child(2)").html("已关闭")
						//$("#openfournet_state p:nth-child(1) a").html("开 通")
					}
				}else{
					$("#openfournet_state span:nth-child(2)").html("无")
				}
			}else{
				$("#openfournet_state span:nth-child(2)").html("无")
			}
		}
}


function open_detail(){
	my_state.init();
	if(menutype != "1100"){
	    detailInfo();
	    //合约期
	    contractPeriod.init();
	    $('#profile-modal').modal('show');
	}
	
}
// 通过返回地区码，网别，付费类型拼接导航栏地址
function load(data) {

	//邮箱登陆非绑定号码 直接跳转到绑定页面
	 if(data && data.menutype && data.menutype =="1100" ){
		 //console.log("data.menutype ="+data.menutype)
		 if(window.parent.location.href.indexOf("/receipt-iframe.html") > -1
			 || window.parent.location.href.indexOf("/query.html") > -1
			 || window.parent.location.href.indexOf("/featuredarea.html") > -1){
		 }else{
			window.parent.location.href = "https://uac.10010.com/cust/bindnum/bindnumInit";
			return;
		 }
	 }
	// 广东省把 办流量包 指向 日租·假日畅游流量包 办理业务
	if (data.pcode == "051") {
		$("#flowpay").show();
	}
	//---这几行是判断登陆的号码是否为西藏的，然后将老用话专区链接替换
	if('079'==data.pcode){
		$('#OldUserLink').attr("href","http://www.10010.com/static/homepage/subjectpage/57900000251770.html");
  	}else{
  		$('#OldUserLink').attr("href","http://www.10010.com/pushpage/59800000169773.html");
  	}
	
	// 登录后导航路径
	var menu_login = "/e4/WT4/WT4_MENU_4_001/";
	if (data.menutype != null && data.menutype != "") {
	 	menutype = data.menutype;
		menu_login = menu_login + data.pcode+'/'+data.nettype+data.pay+'_'+data.menutype+'/';
	} else {
		menu_login = menu_login + data.pcode+'/'+data.nettype+data.pay+'/';
	}
	//如果付费类型为空 则加载未登录的导航
	if(data.pay==null||data.pay==""||data.pay==undefined){
		menu_login=menu_notlogin;
		$("#wrongTc22_title").html("");
		$("#wrongTc22_coment").html("尊敬的客户，出了一点点问题，请您稍候再试或立即反馈我们处理，给您带来不便敬请谅解！");
		$("#wrongTc22_title").parent().find("a").attr("onclick","$('#wrongTc22').hide()");
		$("#wrongTc22_coment").parent().find("input").attr("value","重新登录").attr("onclick","$('#wrongTc22').hide();log_out();");
		_BaseTools.showid("wrongTc22");
	}
	if (data.nettype != '15' && data.menutype !="1100") { // 个人信息  上网卡  邮箱登录未绑定号码 隐藏
		$('#personInfo').attr("onclick","tiaozhuan()");
		$('#menu_userpic').parent().attr('onclick', 'open_detail()');
	} else {
		$('#personInfo').hide();
	}
	$('#menu_query').attr('href',menu_login + 'query.html');
	$('#menu_pay').attr('href',menu_login + 'pay.html');
	$('#menu_hadle').attr('href',menu_login + 'handle.html');
	$('#menu_customer').attr('href',menu_login +'customerService.html');
	$('#menu_service').attr('href',menu_login +'bianminfuwu.html');
	$('#menu_feature').attr('href',menu_login +'featuredarea.html');
	setTimeout(function(){
		$('#bisTran').attr('href',menu_login +'handle.html');
		$('#bisQuery').attr('href',menu_login +'query.html');
		$('#bisFee').attr('href',menu_login +'pay.html');
		$('#bisKefu').attr('href',menu_login +'customerService.html');
		$('#bisTese').attr('href',menu_login +'featuredarea.html');
		$('#bisBianminfuwu').attr('href',menu_login +'bianminfuwu.html');
	},2000)
	
	var url_temp = '';
	// 首页自助登录成条
	$("#bd_li").hide();
	if (data.nettype=='11' || data.nettype=='01' || data.nettype=='02') { // 2,3,4G手机用户
		url_temp = '/e4/index_server.html';
		if(data.menutype == "1100"){
		    $(".left_nav .photo a").attr("onclick","");
		    $(".dropHover #numList").remove();
		}
	} else if (data.nettype=='04' || data.nettype=='05' || data.nettype=='13' || data.nettype=='14') { // 宽带
		url_temp = '/e4/kuandai.html';
		//宽带增加 帐号绑定
		$("#bd_li").show();
	} else if (data.nettype=='03' || data.nettype=='12') { // 固话
		url_temp = '/e4/guhua.html';
	} else { // 上网卡
		url_temp = '/e4/shangwangka.html';
	}
	$('#self-service').attr('href',url_temp);
	$('#bisQuery-self').attr('href',url_temp);
	$('#bisBianminfuwu-self').attr('href',url_temp);
	$('#bisFee-self').attr('href',url_temp);
	$('#bisTran-self').attr('href',url_temp);
	$('#bisTese-self').attr('href',url_temp);
	$('#bisKefu-self').attr('href',url_temp);	
	
	// top_nav.html 自助服务标签事件
	var labelObj = ["#query a","#pay a","#handle a","#customerService a","#bianminfuwu a","#featuredarea a"]; // 自助服务标签
	if(!_BaseTools.isLoginNoshow()) {
		$.each(labelObj,function(i,n){
			$(n).attr("href","javascript:login_modal()");
		});		
	
	} else {
		$.each(labelObj,function(i,n){
			$(n).attr("href",url_temp);
		});	
	}
}
var _HEADER = {
		go : function ()
		{
		window.history.go(-1);
		},
		init:function(){
			$.ajax({
				type:"post",
				url:E3CommonsVariables.RequestPrefix() + "/bind",
				async : true,
				dataType : "JSON",
				beforeSend : function() {
					//$("#loadingGif").show();
					//$("#loadingBg").show();
				},
				complete: closeHeaderMask,
				success:function(data){
					_HEADER.callBack(data);
				}
			});
		},
		callBack:function(data){
			
			if(data==null||data==''|| data.bindnumber == null){
				$('#numList li:contains("号码切换")').remove();
			}else{
					$('#numList li:contains("号码切换")').show();
					$('#numList li').remove(".numChange");
					var numbers = data.bindnumber;
					$.each(numbers, function(idx, rst) {
						
						$('#numList').append('<li class="numChange" onclick="_HEADER.switchMobile(this)"><a href="#">'+rst+'</a></li>');	
					})
					$('#numList').append('<li class="numChange"><a target="_blank" href="https://uac.10010.com/cust/bindnum/bindnumInit">编辑号码</a></li>');
			}
		},
		//切换号码
		switchMobile:function(phoneCode){
			$(phoneCode).parent().hide();
			phoneCode = $.trim($(phoneCode).find('a').html());
			var jut = $.trim($.cookie("JUT"));
			var getUrl = "https://uac.10010.com/portal/SwitchProductCookie?callback=?"; // 现网的地址
			var params = {productID:phoneCode,JsonUserToken:jut};
			$.getJSON(getUrl, params, function(jsonData) {
				//切换成功
				if (jsonData.resultCode) {
					//添加成功后的处理逻辑
					CommonMain.loadData("/l",{param:"switch"},'_HEADER.switchResult(data)');
				} 
				//切换失败
				else {
					window.location.reload();
				}
			});	
		},
		test : function(){
			CommonMain.loadData('/info',null,'load(data)');
		},
		switchResult:function(data){
			setTimeout("test()","4000");
			if(data.result){
				if (data.nettype=='11' || data.nettype=='01' || data.nettype=='02') { // 2,3,4G手机用户
					window.location.href='http://iservice.10010.com/e4/index_server.html';
				} else if (data.nettype=='04' || data.nettype=='05' || data.nettype=='13' || data.nettype=='14') { // 宽带
					window.location.href='http://iservice.10010.com/e4/kuandai.html';
				} else if (data.nettype=='03' || data.nettype=='12') { // 固话
					window.location.href='http://iservice.10010.com/e4/guhua.html';
				} else { // 上网卡
					window.location.href='http://iservice.10010.com/e4/shangwangka.html';
				}
			}else{
				$('#login_footer').hide();
				$('div#layer').remove();
			}
		}
};

function closeHeaderMask (XMLHttpRequest, textStatus){
	//flag变量在commonsBase.js中定义
	//当flag为0时，说明业务数据逻辑层没有加载完成，而这时候用户信息已加载完成，记录flag为1
	if (flag != undefined && flag == "0") {
		flag = "1";
		//记录reqFlag为2时，说明已经加载过用户信息了
		headerReqFlag = "2";
	} else {
		//当reqFlag为2时，说明已经加载过了，这时候只需要还原成1就可以
		if (headerReqFlag == "2") {
			headerReqFlag = "1";
		} else {
			//当第一次请求时，并且flag为1时，说明用户信息已加载完毕，关闭蒙层就可以
			if (!recordFlag) {
				setTimeout("$('#loadingGif,#loadingBg').hide();",1000);
			}
			if (commonReqFlag == "2") {
				if (recordFlag) {
					headerReqFlag = "2";
				} else {
					setTimeout("$('#loadingGif,#loadingBg').hide();",1000);
				}
			}
			if (commonReqFlag == "3") {
				setTimeout("$('#loadingGif,#loadingBg').hide();",1000);
				headerReqFlag = "1";
				commonReqFlag = "1";
			}
		}
	}
}
//切换号码
function numChange(s)
{	 $(s).hide();
	 $(s).siblings().hide();
	 $(s).siblings(".numChange").show()
	
}

