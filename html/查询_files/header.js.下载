});

function showid(idname){
	var isIE = (document.all) ? true : false;
	var isIE6 = isIE&&!window.XMLHttpRequest;
	var newbox = document.getElementById(idname);
	newbox.style.zIndex = "9999999";
	newbox.style.display = "block";
	newbox.style.position = !isIE6 ? "fixed" : "absolute";
	newbox.style.top = newbox.style.left = "50%";
	newbox.style.marginTop = -newbox.offsetHeight / 2 + "px";
	newbox.style.marginLeft = -newbox.offsetWidth / 2 + "px";
	var layer = document.createElement("div");
	layer.id = "layer";
	layer.style.width = layer.style.height = "100%";
	layer.style.position = !isIE6 ? "fixed" : "absolute";
	layer.style.top = layer.style.left = 0;
	layer.style.backgroundColor = "#888";
	layer.style.zIndex = "9999998";
	layer.style.opacity = "0.6";
	document.body.appendChild(layer);

	function layer_iestyle(){
		layer.style.width = Math.max(document.documentElement.scrollWidth, document.documentElement.clientWidth) +
		"px";
		layer.style.height = Math.max(document.documentElement.scrollHeight, document.documentElement.clientHeight) +
		"px";
	}
	function newbox_iestyle(){
		newbox.style.marginTop = document.documentElement.scrollTop - newbox.offsetHeight / 2 + "px";
		newbox.style.marginLeft = document.documentElement.scrollLeft - newbox.offsetWidth / 2 + "px";
	}
	if (isIE) {
		layer.style.filter = "alpha(opacity=60)";
	}
	if (isIE6) {
		layer_iestyle();
		newbox_iestyle();
		window.attachEvent("onscroll", function(){
			newbox_iestyle();
		})
		window.attachEvent("onresize", layer_iestyle);
	}
}
//个人信息展示
function detail(){
	
	detailInfo();
	argUrl = "/searchPerInfoUser/";
	radomT = "?_=" + (new Date()).getTime();
	var url = E3CommonsVariables.RequestPrefixQuery() + argUrl + radomT;
	jQuery.ajax({
		url : url,
		type : "POST",
		data : {},
		async : true,
		dataType : "JSON",
		contentType : "application/x-www-form-urlencoded;charset=UTF-8",
		success : function(data) {
			meNetType=data.userInfo.userNettype;
			meCurrentId = data.userInfo.currentId;
			personal_info_cache = data;	
	   //头部信息
		if(_BaseTools.isLogin()){
		$('#isLogin_out1_id').show();
		$('#isLogin_out2_id').show();
		$('#isnotLog1_out_id').hide();	
		$('#isnotLog2_out_id').hide();
		
		var userName;
		if(data.userInfo.loginType == "05"){
			var v = data.userInfo.currentId.indexOf("@");
			userName = data.userInfo.currentId.substr(0,3) + "*****@" + data.userInfo.currentId.substr(v+1);
		}
		else if(data.userInfo.nickName!=null){
			userName=data.userInfo.nickName;
		}else{
			userName=data.userInfo.mobile;
		}
		// 吐槽手机号
		if(null!=data.userInfo.mobile && "" != data.userInfo.mobile){
			$("#telephone").attr("value",data.userInfo.mobile);
		}
		$('#show_user_name_id').html(userName);
		  }else{
				$('#isLogin_out1_id').hide();
				$('#isLogin_out2_id').hide();
				$('#isnotLog1_out_id').show();	
				$('#isnotLog2_out_id').show();
		  }	
		
			if (null != data && "" != data) {
					if(data.limited == "00"){
						return;
					}
					if(data.limited == "01"){
						return;
					}
					if (null != data.acclimit) {
						if ("true"==data.acclimit) {
							return;
						}
					}
					if( data.ipValidate != null ){
						return;
					}
				}
			//获取用户等级
			if(data.userInfo.userNettype != "" && data.userInfo.userNettype != "fix"){
				if(data.userInfo.custlvl != null && data.userInfo.custlvl !=""){
					custlv1=data.userInfo.custlvl
				}else{
					custlv1="无"
				}
			}else{
				custlv1="无"
			}
			
			//首页
			//首页手机号码
			if(data.userInfo.mobile != "11111111111"){
			    $("#menu_usernumber").html(data.userInfo.mobile+'<span class="caret">');
			}
			if(custlv1== "钻石级"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/purple.png"></i>')
			}else if(custlv1== "金级"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/golden.png"></i>')
			}else if(custlv1== "银级"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/yin.png"></i>')
			}else if(custlv1== "忠诚用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/HEART.png"></i>')
			}else if(custlv1== "贵宾用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/vip.png"></i>')
			}else if(custlv1== "贵宾忠诚用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/vip.png"></i><i><img src="//m2.img.10010.com/e4/images/HEART.png"></i>')
			}else if(custlv1== "一星用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/star.png"></i>')
			}else if(custlv1== "一星忠诚用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/HEART.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i>')
			}else if(custlv1== "一星贵宾用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/vip.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i>')
			}else if(custlv1== "一星贵宾忠诚用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/vip.png"></i><i><img src="//m2.img.10010.com/e4/images/HEART.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i>')
			}else if(custlv1== "二星用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/star.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i>')
			}else if(custlv1== "二星忠诚用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/HEART.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i>')
			}else if(custlv1== "二星贵宾用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/vip.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i>')
			}else if(custlv1== "二星贵宾忠诚用户"){
				$("#menu_custlvl").html('<i><img src="//m2.img.10010.com/e4/images/vip.png"></i><i><img src="//m2.img.10010.com/e4/images/HEART.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i><i><img src="//m2.img.10010.com/e4/images/star.png"></i>')
			}else if(custlv1== "三星用户"){
